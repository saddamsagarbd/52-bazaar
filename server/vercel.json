{
    "version": 2,
    "builds": [
        {
        "src": "server/index.js",
        "use": "@vercel/node",
        "config": {
            "includeFiles": ["server/**/*"]
        }
        }
    ],
    "routes": [
        {
        "src": "/api/(.*)",
        "dest": "/server/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
        }
    ]
}