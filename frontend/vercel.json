{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}