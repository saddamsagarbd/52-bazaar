{
    "version": 2,
    "builds": [
        {
        "src": "server/index.js",
        "use": "@vercel/node",
        "config": {
            "includeFiles": [
            "server/**/*.js",
            "server/**/*.json",
            "server/node_modules/**"
            ],
            "maxDuration": 10
        }
        }
    ],
    "routes": [
        {
        "src": "/api/(.*)",
        "dest": "/server/index.js"
        }
    ]
}